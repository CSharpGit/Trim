<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quan.fems.trim">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:name=".untils.UIApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.CalculatorActivity"/>
        <activity android:name=".activity.ReserveActivity"/>
        <activity android:name=".activity.DesignerListActivity"/>
        <activity android:name=".activity.TrimSceneActivity"/>
        <activity android:name=".activity.DesignerDetailActivity"/>
        <activity android:name=".activity.TrimSceneDetailActivity"/>
    </application>
</manifest>